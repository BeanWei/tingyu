{
  "name": "@tingyu/web",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": ">=16",
    "npm": ">=7",
    "yarn": ">=1"
  },
  "main": "./src/main.tsx",
  "scripts": {
    "reset:vite": "rimraf ./.vite-cache ./dist",
    "dev": "npm run reset:vite && vite",
    "dev:https": "npm run reset:vite && cross-env DEBUG=vite:plugin:mkcert HTTPS=true vite",
    "build": "npm run reset:vite && tsc && vite build",
    "preview": "npm run build && vite preview",
    "prettier": "prettier -c --write \"src/**.{js,jsx,tsx,ts,less,md,json}\"",
    "analyze": "npm run reset:vite && cross-env ANALYZE=true npm run build",
    "mock:server": "json-server --watch ./mock/index.json --config ./mock/config.json"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@types/node": "^18.7.3",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@unocss/vite": "^0.45.6",
    "@vitejs/plugin-react": "^2.0.1",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "json-server": "^0.17.0",
    "less": "^4.1.3",
    "postcss": "^8.4.16",
    "rollup-plugin-visualizer": "^5.7.1",
    "unocss": "^0.45.6",
    "vite": "^3.0.7",
    "vite-plugin-chunk-split": "^0.2.7",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-mkcert": "^1.9.0",
    "vite-plugin-svgr": "^2.2.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "last 10 versions",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
